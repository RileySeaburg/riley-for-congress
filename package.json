{
  "name": "riley-for-congress-html",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch-sass": "node-sass sass/main.scss css/style.css -w",
    "compile-sass": "node-sass sass/main.scss css/style.comp.css",
    "concat-css": "concat -o css/style.concat.css css/icons.css css/style.comp.css",
    "prefix-css": "postcss --user autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress-css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build-css": "npm-run-all compile-sass concat-css prefix-css compress-css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "postcss-cli": "^7.1.0"
  }
}
